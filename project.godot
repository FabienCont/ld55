config_version=5

[application]

config/name="ld55"
run/main_scene="res://menus/home.tscn"
config/features=PackedStringArray("4.2", "GL Compatibility")
boot_splash/bg_color=Color(0.188235, 0.376471, 0.509804, 1)
boot_splash/image="res://assets/background/SplashScreen.png"
config/icon="res://icon.svg"

[autoload]

Signals="*res://scripts/autoload/Signals.gd"
SceneLoader="*res://scenes/autoload/SceneLoader.tscn"
SoundManager="*res://scenes/autoload/SoundManager.tscn"
GlobalManager="*res://scripts/autoload/GlobalManager.gd"

[editor_plugins]

enabled=PackedStringArray("res://addons/JamStarterPack/plugin.cfg", "res://addons/nklbdev.importality/plugin.cfg")

