; Engine configuration file.
; It's best edited using the editor UI and not directly,
; since the parameters that go here are not all obvious.
;
; Format:
;   [section] ; section goes between []
;   param=value ; assign values to parameters

config_version=5

[application]

config/name="ld55"
run/main_scene="res://menus/home.tscn"
config/features=PackedStringArray("4.2", "GL Compatibility")
config/icon="res://icon.svg"

[autoload]

Signals="*res://scripts/autoload/Signals.gd"
SceneLoader="*res://scenes/autoload/SceneLoader.tscn"
SoundManager="*res://scenes/autoload/SoundManager.tscn"
GlobalManager="*res://scripts/autoload/GlobalManager.gd"

[editor_plugins]

enabled=PackedStringArray("res://addons/nklbdev.importality/plugin.cfg")

[file_customization]

folder_colors={
"res://assets/": "blue",
"res://components/": "red",
"res://levels/": "purple",
"res://menus/": "purple",
"res://resources/": "green",
"res://scenes/": "orange",
"res://scripts/": "teal",
"res://shaders/": "pink"
}

[rendering]

renderer/rendering_method="gl_compatibility"
renderer/rendering_method.mobile="gl_compatibility"
